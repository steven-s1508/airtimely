{
	"expo": {
		"name": "Airtimely",
		"slug": "airtimely",
		"scheme": "airtimely",
		"version": "1.0.7",
		"orientation": "portrait",
		"platforms": ["android"],
		"icon": "./src/assets/icon.png",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"splash": {
			"image": "./src/assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#0E9898"
		},
		"web": {
			"bundler": "metro"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./src/assets/adaptive-icon.png",
				"backgroundColor": "#0E9898"
			},
			"edgeToEdgeEnabled": true,
			"package": "com.anonymous.airtimely"
		},
		"plugins": [
			["expo-router"],
			[
				"expo-font",
				{
					"fonts": [
						{
							"fontFamily": "Bebas Neue Pro",
							"fontDefinitions": [
								{
									"path": "./src/assets/fonts/bebas_neue_pro_semi_exp_italic.ttf",
									"weight": "400",
									"style": "italic"
								},
								{
									"path": "./src/assets/fonts/bebas_neue_pro_bold_italic.ttf",
									"weight": "700",
									"style": "italic"
								},
								{
									"path": "./src/assets/fonts/bebas_neue_pro_expanded_medium.ttf",
									"weight": "500",
									"style": "normal"
								},
								{
									"path": "./src/assets/fonts/bebas_neue_pro_expanded_extra_bold.ttf",
									"weight": "800",
									"style": "normal"
								}
							]
						},
						{
							"fontFamily": "Noto Sans",
							"fontDefinitions": [
								{
									"path": "./src/assets/fonts/noto_sans.ttf",
									"weight": "400",
									"style": "normal"
								},
								{
									"path": "./src/assets/fonts/noto_sans_medium.ttf",
									"weight": "500",
									"style": "normal"
								},
								{
									"path": "./src/assets/fonts/noto_sans_bold_italic.ttf",
									"weight": "700",
									"style": "italic"
								}
							]
						}
					]
				}
			],
			"expo-router",
			[
				"@sentry/react-native/expo",
				{
					"url": "https://sentry.io/",
					"project": "airtimely",
					"organization": "airtimely"
				}
			]
		],
		"extra": {
			"router": {},
			"eas": {
				"projectId": "143fc03c-f2f6-4b1a-9035-402c1413088e"
			}
		}
	}
}
